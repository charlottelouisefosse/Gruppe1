@model Gruppe1.Models.PollenRegistering

@{
    ViewData["Title"] = "Slette pollenregistrering";
}

<h1>Slett pollenregistrering</h1>

<h3>Er du sikker p√• at du vil slette denne registreringen?</h3>
<div>
    <h4>Pollenregistrering</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ID)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date)
        </dt>
